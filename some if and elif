num1 = 2**8
if num1 > 128:
    print('correct')

#Zoo treats
animal = 'Bird'
if animal == 'Lion':
    print('meat')
elif animal == 'Zebra':
    print('grass')
elif animal == 'monkey':
    print('fruit')
else:
    print('water')
   
#Dungeon Master code  
choice = input('choose left, right, or straight.')

if choice == 'left':
    print('death by badger')
elif choice == 'right':
    print('death by shark')
elif choice == 'straight':
    print('freedom')
else:
    print('you chose wrong, you die anyways')
    
    
    
#Zoo treats expanded
animal = input('State an animal: ')
if animal == 'Lion' or animal == 'Tiger':
    print('meat')
elif animal == 'Zebra' or animal == 'Bison':
    print('grass')
elif animal == 'monkey':
    print('fruit')
else:
    print('water')
    
    
animal_two = input('tell me an animal: ')
if animal == 'cat':
    print('meow')
elif animal == 'dog':
    print('woof')
else:
    print('boingo')
